<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net452</TargetFrameworks>
      <Version>1.0.0</Version>
      <Authors>Bernhard Richter</Authors>
      <PackageProjectUrl>http//www.lightinject.net</PackageProjectUrl>
      <RepositoryType>git</RepositoryType>
      <RepositoryUrl>https://github.com/seesharper/LightInject.NServiceBus</RepositoryUrl>
      <PackageLicenseUrl>http://opensource.org/licenses/MIT</PackageLicenseUrl>
      <Copyright>Bernhard Richter</Copyright>
      <PackageTags>Ioc Dependency-Injection Inversion-of-Control NServiceBus</PackageTags>
      <Description>Allows LightInject to be used as an external container with NServiceBus. See LightInject.NServiceBus.Source for the source distribution.</Description>
      <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
      <DebugType>Full</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="LightInject" Version="5.0.3" />
    <PackageReference Include="NServiceBus" Version="6.3.4" />
  </ItemGroup>
</Project>